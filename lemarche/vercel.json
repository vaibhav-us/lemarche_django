{
    "builds": [
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
          },
        {
        "src": "lemarche/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "lemarche/wsgi.py"
        },
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        }
    ]
}